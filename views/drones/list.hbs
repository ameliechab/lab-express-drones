<h2>Available drones</h2>

<ul>
  {{#each allDrones}}
    <li>
      <p>{{this.name}}</p>
      <p>{{this.propellers}}</p>
      <p>{{this.maxSpeed}}</p>
      <a href="/drones/{{this._id}}/update">Edit</a>

      <form action="/drones/{{this._id}}/delete" method="post">
        <button>Delete</button>
      </form>
    </li>

  {{/each}}
</ul>